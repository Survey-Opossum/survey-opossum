<h1>Survey Results</h1>
<br>
<%= @survey.title %><br>
<%= @survey.description %>


<% @survey.questions.each do |q| %>
    <div> <%= q.text %>: </div>
      <% q.answers.each do |a| %>
        <div><%= a.text %></div>
      <% end %>
<% end %>
<br>
<%= link_to '</button type="button">Back</button>' .html_safe, surveys_path %>

<h1>Survey Opossum</h1>

<%= form_tag sessions_path do %>
  <%= text_field_tag "email", nil, placeholder: "email" %><br>
  <%= password_field_tag "password", nil, placeholder: "password"  %><br>
<%= submit_tag "Login" %><%= link_to '<button type="button">Sign up</button>' .html_safe, new_author_path %>

<% end %>
